{
    "name": "akash/framework",
    "description": "ZestPHP: High-performance PHP MVC Framework with React",
    "type": "project",
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "ext-json": "*",
        "symfony/http-foundation": "^6.0",
        "symfony/routing": "^6.0",
        "league/container": "^4.0",
        "nikic/fast-route": "^1.3",
        "vlucas/phpdotenv": "^5.0",
        "monolog/monolog": "^3.0",
        "symfony/mailer": "^6.0",
        "symfony/mime": "^6.0",
        "illuminate/database": "^10.0",
        "symfony/console": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/"
        },
        "files": [
            "core/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
